{
  "source": {
    "include": ["src/controllers", "src/services", "src/routes"],
    "includePattern": ".js$"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}